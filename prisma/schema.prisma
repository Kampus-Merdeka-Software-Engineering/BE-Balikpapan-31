// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pasien {
  id        Int      @id @default(autoincrement())
  nama      String
  email     String
  no_hp     String
  nama_dokter String
  pesan     String
  created_at DateTime @default(now())
}